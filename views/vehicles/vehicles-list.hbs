<form action=''>
  <div class='set-of-vehicles'>
    {{#each vehiclesFromApi}}
      <div class='vehicle-card'>
        <img class='vehicle-img' src="{{this.primaryPhotoUrl}}" alt=''/>
        <div>
          <span>{{this.year}}</span>
          <span>{{this.make}}</span>
          <span>{{this.model}}</span>
          <span>{{this.price}}</span>
          <span>{{this.mileage}}</span>
          <span><h4><b>Exterior</b></h4>{{this.displayColor}}</span>
          <span><h4><b>Location</b></h4>{{this.city}}, {{this.state}}</span>
        </div>
      </div>
    {{/each}}

    {{#if errorMessage}}
      {{errorMessage}}
    {{/if}}
  </div>
</form>