<div class='user-profile'>
  <div class="user-image-top-left">
    <img class="user-image" src="{{userObject.profilePic}}" alt="image of user" />

  </div>
  <div class='user-details'>
    {{!-- <p>{{userObject.firstName}}</p> --}}
    <h3>Name:</h3> <h2> {{userObject.firstName}} {{userObject.lastName}}</h2>
    <br>
    <h3>Location:</h3> <h2>{{userObject.location}}</h2>
    <br>
    <h3>Current Vehicle:</h3> <h2>{{userObject.currentVehicle}}</h2>
    <br>
    <h4><a href ="/profile/edit">Edit Profile</a></h4>
  </div>

  <div class="user-listings">
    <h3>My Listings</h3>
    <h2>
    <li><a href="">Listing 1</a></li>
    <li><a href="">Listing 2</a></li>
    </h2>
  </div>

  <div class="saved-vehicles">
    <h3><a href="/profile/savedvehicles">Saved Vehicles</a></h3>
  </div>

</div>